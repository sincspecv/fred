{
  "name": "fred",
  "version": "0.1.2",
  "description": "Fred - A flexible framework for building AI agents with intent-based routing",
  "type": "module",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "default": "./src/index.ts"
    }
  },
  "files": [
    "lib",
    "src"
  ],
  "scripts": {
    "dev": "bun run src/dev-chat.ts",
    "server": "bun run src/server.ts",
    "build": "bun build src/index.ts --outdir lib --target bun --format esm",
    "prepublishOnly": "bun run build",
    "test": "bun test",
    "test:unit": "bun test tests/unit",
    "test:all": "bun test",
    "docs:dev": "mkdocs serve",
    "docs:build": "mkdocs build",
    "docs:deploy": "mkdocs gh-deploy"
  },
  "bin": {
    "fred": "./src/cli/index.ts"
  },
  "keywords": [
    "ai",
    "agents",
    "framework",
    "intent",
    "vercel-ai-sdk"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "ai": "^6.0.0",
    "js-yaml": "^4.1.0"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.9",
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "@ai-sdk/openai": "*",
    "@ai-sdk/groq": "*",
    "@ai-sdk/anthropic": "*",
    "@ai-sdk/google": "*",
    "@ai-sdk/mistral": "*",
    "@ai-sdk/cohere": "*",
    "@ai-sdk/vercel": "*",
    "@ai-sdk/fireworks": "*",
    "@ai-sdk/xai": "*",
    "@ai-sdk/amazon-bedrock": "*",
    "@ai-sdk/elevenlabs": "*",
    "@ai-sdk/perplexity": "*",
    "@ai-sdk/replicate": "*"
  },
  "peerDependenciesMeta": {
    "@ai-sdk/openai": {
      "optional": true
    },
    "@ai-sdk/groq": {
      "optional": true
    },
    "@ai-sdk/anthropic": {
      "optional": true
    },
    "@ai-sdk/google": {
      "optional": true
    },
    "@ai-sdk/mistral": {
      "optional": true
    },
    "@ai-sdk/cohere": {
      "optional": true
    },
    "@ai-sdk/vercel": {
      "optional": true
    },
    "@ai-sdk/fireworks": {
      "optional": true
    },
    "@ai-sdk/xai": {
      "optional": true
    },
    "@ai-sdk/amazon-bedrock": {
      "optional": true
    },
    "@ai-sdk/elevenlabs": {
      "optional": true
    },
    "@ai-sdk/perplexity": {
      "optional": true
    },
    "@ai-sdk/replicate": {
      "optional": true
    }
  }
}

